
lunaix-root := $(LUNAIX_ROOT)
unit-test-root := $(lunaix-root)/tests/units
CFLAGS += -idirafter $(lunaix-root)/includes	\
		  -isystem $(unit-test-root)/stubs/includes \
		  -Wno-discarded-qualifiers \
		  -g

include $(lunaix-root)/makeinc/utils.mkinc

%.o: %.c
	$(call status,CC,$(@F))
	@$(CC) $(CFLAGS) -c $< -o $@