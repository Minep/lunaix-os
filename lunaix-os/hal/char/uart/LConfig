
@"16x50 Serial Controller"
@(parent := char_device)
def uart_16x50():
    """ 16x50 serial controller  """

    @"16x50 XT-Compat"
    def xt_16x50() -> bool:
        """ Enable the 16x50 for PC-compatible platform  """
        require(arch_x86)
        
        return True
    
    @"16x50 PCI"
    def pci_16x50() -> bool:
        """ Enable the support of PCI 16x50 """

        return True